{
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 10000
    },
    "firestore": {
      "port": 10001
    },
    "database": {
      "port": 10002
    },
    "hosting": {
      "port": 10003
    },
    "ui": {
      "enabled": true,
      "port": 10004
    }
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  }
}
