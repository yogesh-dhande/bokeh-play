{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "firebase": "npm run default && firebase emulators:start --import test/.seed",
    "export": "firebase emulators:export test/.seed",
    "default": "firebase use staging",
    "prod": "firebase use production",
    "start": "nuxt start",
    "build": "rm -rf dist && nuxt generate",
    "deploy": "npm run prod && DEPLOY_TARGET=production firebase deploy --only=functions,firestore,storage",
    "test:firebase": "firebase emulators:exec 'jest --env=node --detectOpenHandles --watch test/firebase/*'",
    "test": "jest",
    "reinstall": "rm package-lock.json && rm -rf node_modules && npm i"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "@splitbee/web": "^0.3.0",
    "axios": "^0.21.4",
    "core-js": "^3.9.1",
    "firebase": "^9.6.0",
    "monaco-editor": "^0.24.0",
    "monaco-languageclient": "0.13.1-next.9",
    "nuxt": "^2.13.3",
    "postscribe": "^2.0.8",
    "vscode-ws-jsonrpc": "^0.2.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "~2.0.1",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.0.1",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "jest": "^26.6.3",
    "monaco-editor-webpack-plugin": "^3.0.0",
    "postcss": "^8.2.8",
    "prettier": "^2.3.0",
    "vue-jest": "^3.0.4"
  }
}
