:8080 {
    handle /server/* {
        reverse_proxy http://server:8080
    }
    
    handle /lsp/* {
        reverse_proxy http://lsp:8080
    }

    # Hack to serve static panel resources because they panel serve does not respect --prefix
    handle /static/extensions/panel/* {
        reverse_proxy http://server:8080
    }

    handle * {
        reverse_proxy http://client:8080
    }
    
}